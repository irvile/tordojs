{
  "name": "@tordojs/monorepo",
  "private": true,
  "scripts": {
    "build:clean": "lerna run build:clean --no-private",
    "build:esm": "lerna run build:esm --no-private",
    "build:types": "lerna run build:types --no-private",
    "build:umd:cjs": "lerna run build:umd:cjs --no-private",
